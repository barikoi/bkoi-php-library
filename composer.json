{
    "name": "barikoi/barikoi-api",
    "description": "A comprehensive Laravel package for integrating Barikoi API - Bangladesh's leading location data provider",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "barikoi", "geocoding", "maps", "location", "bangladesh", "routing", "geofencing"],
    "authors": [
        {
            "name": "Barikoi",
            "email": "info@barikoi.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\BarikoiApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\BarikoiApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:verbose": "phpunit --testdox",
        "test:detailed": "phpunit --testdox --colors=always",
        "test:step": "php run-tests-step-by-step.php",
        "test:filter": "phpunit --filter",
        "test:feature": "phpunit --testsuite Feature --testdox --colors=always",
        "test:unit": "phpunit --testsuite Unit --testdox --colors=always",
        "test:integration": "phpunit --testsuite Integration --testdox --colors=always --group integration",
        "test-coverage": "phpunit --coverage-html coverage",
        "test-coverage-text": "phpunit --coverage-text"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\BarikoiApi\\BarikoiServiceProvider"
            ],
            "aliases": {
                "Barikoi": "Vendor\\BarikoiApi\\Facades\\Barikoi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
